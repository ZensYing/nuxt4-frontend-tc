{
  "buildCommand": "nuxt build",
  "outputDirectory": ".output/public",
  "installCommand": "yarn install --frozen-lockfile",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "handler": ".output/server/index.mjs"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
